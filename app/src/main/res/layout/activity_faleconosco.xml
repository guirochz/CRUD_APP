<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fundo">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:text="@string/fale"
        android:textColor="#FFFFFF"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/containerComponents2"
        style="@style/ContainerComponents"
        android:layout_width="364dp"
        android:layout_height="400dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="316dp"
        android:layout_height="65dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Tem alguma dúvida, reclamação ou elogio?"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="@+id/containerComponents2"
        app:layout_constraintStart_toStartOf="@+id/containerComponents2"
        app:layout_constraintTop_toTopOf="@+id/containerComponents2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_marginBottom="28dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Contate-nos"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/containerComponents2"
        app:layout_constraintStart_toStartOf="@+id/containerComponents2"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="270dp"
        android:layout_height="65dp"
        android:backgroundTint="#FFFFFF"
        android:hint="Digite aqui sua duvida"
        android:textColorHint="#FFFFFF"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/containerComponents2"
        app:layout_constraintStart_toStartOf="@+id/containerComponents2"
        app:layout_constraintTop_toBottomOf="@+id/textView3"/>

    <Button
        android:id="@+id/button"
        android:layout_width="179dp"
        android:layout_height="59dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="@color/fundo"
        android:text="Enviar "
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_marginTop="20dp"
        android:onClick="abrindo_faleconosco"
        app:layout_constraintEnd_toEndOf="@+id/containerComponents2"
        app:layout_constraintStart_toStartOf="@+id/containerComponents2"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress"/>

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="200dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/ic_home3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:onClick="abrindo_perfil"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="320dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="15dp"
            android:src="@drawable/ic_user" />

        <ImageView
            android:id="@+id/ic_home"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:onClick="abrindo_home"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="37dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:src="@drawable/ic_home" />

        <ImageView
            android:id="@+id/ic_home2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:onClick="abrindo_favoritos"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="181dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:src="@drawable/ic_heart" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>